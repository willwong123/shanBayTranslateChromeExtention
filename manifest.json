{
    "manifest_version": 2,
    "name": "扇贝翻译",
    "version": "0.1",
    "description": "翻译你所不认识的单词",
    "content_scripts": [
        {
            "matches": ["http://*/*","https://*/*"],
            "js": ["js/main.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "扇贝翻译",
		"default_popup": ""
	}
}
